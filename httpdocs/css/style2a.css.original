/** directions page - Google Maps API styles **/
#panel {
    position: absolute;
    top: 5px;
    left: 50%;
    margin-left: -180px;
    z-index: 5;
    background-color: #fff;
    padding: 5px;
    border: 1px solid #999;
}

#directions-panel {
    /*height: auto;*/
    margin: -10px 0px 0px 0px;
    float: right;
    width: 480px;
    overflow: auto;
    display: none;
}

#pre-directions-panel {
    position: absolute;
    /*height: 100%;*/
    margin: 0px 30px 0px 500px;
    float: right;
    width: 470px;
    z-index: 0;
    overflow: auto;
    display: none;
}      

#map-canvas {
    width: 500px;
    height: 700px;        
    margin: 0 0 0 -15px;
    padding: 0px;
}

#control {
    background: #fff;
    padding: 5px;
    font-size: 14px;
    font-family: Arial;
    border: 1px solid #ccc;
    box-shadow: 0 2px 2px rgba(33, 33, 33, 0.4);
    display: none;
}
#control strong
{
    margin: 0 0 0 110px;
}    
#control input#start {
    margin: 0 0 0 100px; 
    width: 260px;
}  
#control button#getDirections {
    margin: 0 0 0 180px;
}     

#pre-directions-panel img {
    width: 100%;
    height: 30px;
    padding: 5% 0; 
    float: left;
}
#pre-directions-panel h2 {
    text-align: center; 
    clear: both;
}

