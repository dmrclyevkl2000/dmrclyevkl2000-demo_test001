/* footer */
#footer
{
    background: none repeat scroll 0 0 #FFFF99;
    border-radius: 10px;
    clear: both;
    height: 40px;
    padding: 0;
    margin: 10px 0;
    text-align: left;
}
#footer-left
{
    padding:10px 0 0 0;
    float:left;
    width: auto;
    display: inline;
}
#footer-right
{ 
    margin:0.5% 10px 0 0;	
    color:#000;	
    float:right; 
    padding:0px;	
    display: inline;	
    font-size: 0.7em;
}
#footer-legal
{ 
    margin:0;
    color:#000;	
    float:right; 
    padding:0px 10px 0 0;	
    display: inline;	
    font-size: 0.6em;
}

#footer-left ul
{
    display:inline;
    width:720px;
    margin:0;
    list-style:none;
    float:left;
    padding-left:0px;
    text-align:center;
}

#footer-left a
{
    display:block;
    /*	text-decoration:none;*/
    color:#000;
    width:auto;
    min-width:50px;
    float:left;
    padding:0px 15px;
    border-right:1px solid #fff;
}
#footer-left li.small {
    font-size: 0.42em;
    display:inline;
    width:220px;
    margin:0;
    list-style:none;
    float:right;
    border:none;
}

#footer-left li.last a{
    display:inline;
    width:220px;
    margin:0;
    list-style:none;
    float:left;
    border:none;
}

#footer-left li {
    display:inline;
}

